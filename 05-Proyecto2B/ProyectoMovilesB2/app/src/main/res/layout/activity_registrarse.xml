                <TextView

                    style="@style/txtRobotoromanbold36"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_cinemeter" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearForm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_66pxv"
                android:layout_marginBottom="@dimen/_5pxv"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/linearForm1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageIconuserOne"
                        android:layout_width="@dimen/_26pxh"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_gravity="center_vertical"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_icon_user"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_icon_user" />

                    <EditText
                        android:id="@+id/etUsuario123value"
                        style="@style/etSolidRounded"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:background="@drawable/rectangle_bg_gray_800_radius_8"
                        android:ellipsize="end"
                        android:hint="@string/lbl_usuario123"
                        android:inputType="text"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_16pxh"
                        android:paddingBottom="@dimen/_8pxv"
                        android:singleLine="true"
                        android:textColorHint="@color/lime_100"
                        tools:ignore="Autofill" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearForm2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageIconmailOne"
                        android:layout_width="@dimen/_26pxh"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_gravity="center_vertical"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_icon_mail"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_icon_mail" />

                    <EditText
                        android:id="@+id/etEmail2"
                        style="@style/etSolidRounded"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:background="@drawable/rectangle_bg_gray_800_radius_8"
                        android:ellipsize="end"
                        android:hint="@string/msg_example_example_com"
                        android:inputType="text"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_16pxh"
                        android:paddingBottom="@dimen/_8pxv"
                        android:singleLine="true"
                        android:textColorHint="@color/lime_100"
                        tools:ignore="Autofill" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearForm3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageIconlcokOne"
                        android:layout_width="@dimen/_26pxh"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_gravity="center_vertical"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_icon_lcok"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_icon_lcok" />

                    <EditText
                        android:id="@+id/etContraseavalue"
                        style="@style/etSolidRounded"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:background="@drawable/rectangle_bg_gray_800_radius_8"
                        android:ellipsize="end"
                        android:hint="@string/lbl_contrase_a"
                        android:inputType="text"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_16pxh"
                        android:paddingBottom="@dimen/_8pxv"
                        android:singleLine="true"
                        android:textColorHint="@color/lime_100"
                        tools:ignore="Autofill" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearForm4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageIconlcokThree"
                        android:layout_width="@dimen/_26pxh"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_gravity="center_vertical"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_icon_lcok"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_icon_lcok" />

                    <EditText
                        android:id="@+id/etInput"
                        style="@style/etSolidRounded"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:background="@drawable/rectangle_bg_gray_800_radius_8"
                        android:ellipsize="end"
                        android:hint="@string/msg_confirmar_contrase_a"
                        android:inputType="text"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_16pxh"
                        android:paddingBottom="@dimen/_8pxv"
                        android:singleLine="true"
                        android:textColorHint="@color/lime_100"
                        tools:ignore="Autofill" />
                </LinearLayout>

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btnRegistrarse"
                    style="@style/btnSolidRounded"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_8pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_registrarse"
                    android:textAllCaps="false"
                    tools:text="@string/lbl_registrarse" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>