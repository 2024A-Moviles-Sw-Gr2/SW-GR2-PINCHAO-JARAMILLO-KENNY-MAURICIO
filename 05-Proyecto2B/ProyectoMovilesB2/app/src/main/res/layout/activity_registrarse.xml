<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Registrarse">
    <LinearLayout
        android:id="@+id/linearAndroidSmall2"
        style="@style/groupStylegray_900"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <androidx.appcompat.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_15pxv"
            android:gravity="center_horizontal"
            app:contentInsetStart="0dp">

            <LinearLayout
                android:id="@+id/regresar_registrarse"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_marginEnd="@dimen/_244pxh"
                android:layout_marginBottom="@dimen/_14pxv"
                android:orientation="horizontal">

                <ImageView

                    android:layout_width="@dimen/_26pxh"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_gravity="center_vertical"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_left"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_arrow_left" />

                <TextView
                    style="@style/txtRobotoromanregular20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_login" />
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>

        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_57pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingBottom="@dimen/_57pxv">

            <LinearLayout
                android:id="@+id/linearLogorow2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginEnd="@dimen/_21pxh"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <FrameLayout
                    android:id="@+id/frameStackvectorOne"
                    android:layout_width="@dimen/_69pxh"
                    android:layout_height="@dimen/_52pxv">

                    <ImageView
                        android:id="@+id/imageVectorOne"
                        android:layout_width="@dimen/_43pxh"
                        android:layout_height="@dimen/_52pxh"
                        android:layout_gravity="start"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_vector" />

                    <ImageView
                        android:id="@+id/imageVectorThree"
                        android:layout_width="@dimen/_12pxh"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_gravity="top|end"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_marginEnd="@dimen/_9pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_yellow_700"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_vector_yellow_700" />

                    <ImageView
                        android:id="@+id/imageVectorFive"
                        android:layout_width="@dimen/_26pxh"
                        android:layout_height="@dimen/_43pxh"
                        android:layout_gravity="top|end"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_yellow_700_43x26"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_vector_yellow_700_43x26" />

                    <ImageView
                        android:id="@+id/imageVectorSeven"
                        android:layout_width="@dimen/_14pxh"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_gravity="top|end"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_yellow_700_13x14"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_vector_yellow_700_13x14" />

                    <ImageView
                        android:id="@+id/imageVectorNine"
                        android:layout_width="@dimen/_22pxh"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_gravity="top|end"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_yellow_700_36x22"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_vector_yellow_700_36x22" />

                    <ImageView
                        android:id="@+id/imageVectorEleven"
                        android:layout_width="@dimen/_16pxh"
                        android:layout_height="@dimen/_8pxh"
                        android:layout_gravity="bottom|end"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_yellow_700_8x16"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_vector_yellow_700_8x16" />

                    <ImageView
                        android:id="@+id/imageVectorThirteen"
                        android:layout_width="@dimen/_7pxh"
                        android:layout_height="@dimen/_9pxh"
                        android:layout_gravity="bottom|start"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_gray_900"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_vector_gray_900" />

                    <LinearLayout
                        android:id="@+id/linearColumnvector1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="top|start"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_marginEnd="@dimen/_45pxh"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/linearRowvector"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="end"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageVectorFifteen"
                                android:layout_width="@dimen/_6pxh"
                                android:layout_height="@dimen/_8pxh"
                                android:layout_gravity="bottom"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector_gray_900_8x6"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_vector_gray_900_8x6" />

                            <ImageView
                                android:id="@+id/imageVector"
                                android:layout_width="@dimen/_6pxh"
                                android:layout_height="@dimen/_9pxh"
                                android:layout_gravity="top"
                                android:layout_marginStart="@dimen/_4pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector_gray_900_8x6"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_vector_gray_900_8x6" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linearRowvector1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageVectorNineteen"
                                android:layout_width="@dimen/_6pxh"
                                android:layout_height="@dimen/_8pxh"
                                android:layout_gravity="bottom"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector_gray_900_8x6"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_vector_gray_900_8x6" />

                            <ImageView
                                android:id="@+id/imageVector1"
                                android:layout_width="@dimen/_4pxh"
                                android:layout_height="@dimen/_5pxh"
                                android:layout_gravity="top"
                                android:layout_marginStart="@dimen/_4pxh"
                                android:layout_marginBottom="@dimen/_4pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector_gray_900_5x4"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_vector_gray_900_5x4" />
                        </LinearLayout>
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/imageVector2"
                        android:layout_width="@dimen/_6pxh"
                        android:layout_height="@dimen/_8pxh"
                        android:layout_gravity="top|start"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_8x6"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_vector_8x6" />

                    <ImageView
                        android:id="@+id/imageVector3"
                        android:layout_width="@dimen/_6pxh"
                        android:layout_height="@dimen/_8pxh"
                        android:layout_gravity="bottom|start"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_gray_900_8x6"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_vector_gray_900_8x6" />
